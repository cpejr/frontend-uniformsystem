funcionario);\r\n      }\r\n    });\r\n\r\n    // Se nao tiver nada no Input de busca, cooca todos\r\n    if (employee_name === \"\") {\r\n      setFuncionarioFiltrado([...employees]);\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <MetaData\r\n        titlePage={meta.titlePage}\r\n        titleSearch={meta.titleSearch}\r\n        description={meta.description}\r\n        keyWords={meta.keyWords}\r\n        imageUrl={meta.imageUrl}\r\n        imageAlt={meta.imageAlt}\r\n      />\r\n      <div className=\"topEmployee\">\r\n        <div className=\"searchEmployee\">\r\n          <input\r\n            id=\"searchEmployee\"\r\n            type=\"text\"\r\n            ref={inputSearch}\r\n            placeholder=\"Buscar Funcionário\"\r\n          />\r\n\r\n          <FaSearch onClick={FilterEmployee} className=\"searchButtonEmployee\" />\r\n        </div>\r\n        <div>\r\n          <Link className=\"buttonEmployee\" to=\"/adm/funcionarios/cadastro\">\r\n            <Button type=\"button\">CADASTRAR FUNCIONÁRIO</Button>\r\n          </Link>\r\n        </div>\r\n      </div>\r\n      <TableContainer component={Paper}>\r\n        <Table\r\n          className={classes.table}\r\n          size=\"small\"\r\n          aria-label=\"a dense table\"\r\n        >\r\n          <TableHead>\r\n            <TableRow>\r\n              <TableCell align=\"center\" className=\"header-table\">\r\n                NOME COMPLETO\r\n              </TableCell>\r\n              <TableCell align=\"center\" className=\"header-table\">\r\n                EMAIL\r\n              </TableCell>\r\n              <TableCell align=\"center\" className=\"header-table\">\r\n                AÇÕES\r\n              </TableCell>\r\n            </TableRow>\r\n          </TableHead>\r\n          <TableBody>\r\n            {funcionarioFiltrado.length > 0 ? (\r\n              funcionarioFiltrado.map((employee) => {\r\n                const id = employee.user_id;\r\n                const colum = (\r\n                  <TableRow key={employee.user_id}>\r\n                    <TableCell component=\"td\" scope=\"row\">\r\n                      {employee.name}\r\n                    </TableCell>\r\n                    <TableCell component=\"td\" scope=\"row\">\r\n                      {employee.email}\r\n                    </TableCell>\r\n                    <TableCell\r\n                      component=\"td\"\r\n                      scope=\"row\"\r\n                      className={classes.actions}\r\n                    >\r\n                      {id !== user[0].user_id ? (\r\n                        <IconButton onClick={() => handleOpen(employee)}>\r\n                          <BsFillTrashFill />\r\n                        </IconButton>\r\n                      ) : null}\r\n                      <IconButton>\r\n                        <Link to={`/adm/funcionario/` + id}>\r\n                          <BsInfoCircle />\r\n                        </Link>\r\n                      </IconButton>\r\n                    </TableCell>\r\n                  </TableRow>\r\n                );\r\n                return colum;\r\n              })\r\n            ) : (\r\n              <span>Nenhum funcionário cadastrado</span>\r\n            )}\r\n          </TableBody>\r\n        </Table>\r\n      </TableContainer>\r\n\r\n      <ExcludeDialog\r\n        open={dialogItem.open}\r\n        handleClose={handleClose}\r\n        title={dialogItem.item?.name}\r\n        callback={deleteEmployee}\r\n      />\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default EmployeeAdm;\r\n","D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\Pages\\Administrador\\ProductsAdm\\EditProduct\\EditProduct.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\HeaderAdm\\Mobile_HeaderAdm\\MobileHeaderAdm.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\Skeletons\\MobileShopSkeleton.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\DadosPessoais\\DadosPessoais.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\ProductCard\\index.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\ProductCardAdm\\index.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\ProductCard\\ProductCard.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\CardPedido\\CardPedido.js",["268"],"import React, { useEffect, useState, useMemo } from \"react\";\r\nimport \"./CardPedido.css\";\r\nimport { MdInfoOutline } from 'react-icons/md';\r\nimport { RiTruckFill } from 'react-icons/ri';\r\nimport { Button } from 'react-bootstrap';\r\n\r\nimport api from '../../services/api';\r\n\r\nfunction formatOrderStatus(status){\r\n  let statusInPortuguese;\r\n  switch (status) {\r\n    case 'waitingPayment':\r\n      statusInPortuguese = 'Aguardando Pagamento';\r\n      break;\r\n    case 'pending':\r\n      statusInPortuguese = 'Pendente';\r\n      break;\r\n    case 'preparing':\r\n      statusInPortuguese = 'Preparando';\r\n      break;\r\n    case 'delivering':\r\n      statusInPortuguese = 'Em entrega';\r\n      break;\r\n    default:\r\n      break;\r\n  }\r\n  return statusInPortuguese;\r\n}\r\n\r\n\r\nfunction CardPedido({pedido, token}){\r\n\r\n    const [productsFromOrder, setProductsFromOrder] = useState([]);\r\n\r\n    const orderDate = new Date(pedido.created_at);\r\n    const dayOrder = orderDate.getDate();\r\n    const monthOrder = orderDate.getMonth() + 1;\r\n    const yearOrder = orderDate.getFullYear();\r\n\r\n    const statusFormatted = formatOrderStatus(pedido.status);\r\n\r\n    useEffect(() => {\r\n\r\n      async function getProductsFromOrder(){\r\n        const response = await api.get(`/productsfromorder/${pedido.order_id}`,\r\n        {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        },\r\n        );\r\n\r\n        console.log('products from order', response);\r\n        if(response.data){\r\n          setProductsFromOrder(response.data);\r\n        }\r\n      }\r\n\r\n      getProductsFromOrder();\r\n\r\n    }, []);\r\n\r\n    const totalPriceOrder = useMemo(() => {\r\n      let totalAux = 0;\r\n      productsFromOrder.forEach( item => {\r\n        totalAux += ((item.product_price * item.amount) - item.discount)\r\n        return totalAux;\r\n      });\r\n\r\n      console.log('aqui', totalAux)\r\n\r\n      return totalAux;\r\n    }, [productsFromOrder]);\r\n\r\n    return (\r\n      <div className=\"pedido\">\r\n        <div className=\"pedidoNumber\">ID: {pedido.order_id}</div>\r\n        <div className=\"pedidoData\">Data do pedido: {dayOrder}/{monthOrder}/{yearOrder}</div>  \r\n        <hr className=\"horizontalLine\"></hr>\r\n        <div className=\"pedidoStatus\">\r\n          <MdInfoOutline style={{fontSize:'22px', marginRight: '7px'}}/>\r\n          <span>Status: {statusFormatted}</span>\r\n        </div>  \r\n        <div className=\"pedidoDestino\">\r\n          <RiTruckFill style={{fontSize:'22px', marginRight: '7px'}}/>\r\n          <span>Destino: {pedido.city}/{pedido.state} - {pedido.zip_code}</span>\r\n        </div>  \r\n        <div className=\"pedidoTotal\">Total: R$ {totalPriceOrder.toFixed(2)}</div>\r\n        {\r\n          pedido.status === \"delivered\" && \r\n          <>\r\n            <span className=\"trackingCode\">Cod. Rastreamento: {pedido.tracking_code}</span>  \r\n            <Button className=\"pedidoBotao2\">\r\n              <a href=\"https://www2.correios.com.br/sistemas/rastreamento/default.cfm\" target=\"_blank\" rel=\"noreferrer\" >\r\n                Acompanhar pedido\r\n              </a>\r\n            </Button>\r\n          </>\r\n        }\r\n      </div>\r\n    );\r\n\r\n}\r\n\r\nexport default CardPedido;\r\n\r\n\r\n\r\n\r\n\r\n","D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\PopUpProductModel\\PopUpProductModel.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\index.js",[],["269","270"],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\Pages\\Perfil\\index.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\Pages\\Loja\\index.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\Pages\\Carrinho\\index.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\Pages\\Checkout\\index.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\services\\auth.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\Pages\\Administrador\\OrdersAdm\\EspecificOrderAdm\\EspecificOrderAdm.js",["271","272"],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\FooterAdm\\FooterAdm.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\SidebarClient\\SidebarClient.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\meta\\reactHelmet.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\Skeletons\\ProductSkeleton.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\OrderTable\\OrderTable.js",[],"D:\\Documents\\Arthur\\UFMG\\CPEjr\\Familia_pera\\Uniform_Sytem\\frontend-uniformsystem\\src\\components\\CardPedido\\index.js",[],"D: