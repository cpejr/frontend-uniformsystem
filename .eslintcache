oyee}\r\n        helperText={errorTypeEmployeeMessage}\r\n        className={classes.inputText}\r\n        onChange={(e) => handleChangeTypeEmployee(e)}\r\n        variant=\"outlined\"\r\n      >\r\n        <MenuItem value=\"\">Selecione uma opção</MenuItem>\r\n        <MenuItem value=\"employee\">Funcionário</MenuItem>\r\n        <MenuItem value=\"adm\">Adminstrador</MenuItem>\r\n      </TextField>\r\n\r\n      <div className={classes.divButtons}>\r\n        <Button className={classes.saveButton} onClick={() => handleSubmit()}>\r\n          {loading ? <CircularProgress color=\"secondary\" /> : \"CADASTRAR\"}\r\n        </Button>\r\n      </div>\r\n\r\n      <Snackbar\r\n        open={openSnackBar}\r\n        autoHideDuration={5000}\r\n        onClose={handleCloseSnackBar}\r\n      >\r\n        <MuiAlert\r\n          onClose={handleCloseSnackBar}\r\n          elevation={6}\r\n          variant=\"filled\"\r\n          severity=\"success\"\r\n        >\r\n          Funcionário cadastrado com sucesso!\r\n        </MuiAlert>\r\n      </Snackbar>\r\n    </div>\r\n  );\r\n}\r\n\r\nconst useStyles = makeStyles((theme) => ({\r\n  icon: {\r\n    position: \"absolute\",\r\n    fontSize: \"35px\",\r\n    top: \"10px\",\r\n    left: \"5px\",\r\n    color: \"#666\",\r\n    cursor: \"pointer\",\r\n    \"&:hover\": {\r\n      color: \"#007bff\",\r\n    },\r\n  },\r\n  mainTitle: {\r\n    width: \"fit-content\",\r\n    fontSize: \"32px\",\r\n    lineHeight: \"49px\",\r\n    marginTop: \"35px\",\r\n    marginBottom: \"30px\",\r\n    display: \"flex\",\r\n    flexDirection: \"column\",\r\n  },\r\n  spanInsideTitle: {\r\n    width: \"90%\",\r\n    height: \"2px\",\r\n    margin: \"0 auto\",\r\n    borderBottom: \"2px solid #0EC4AB\",\r\n  },\r\n  inputText: {\r\n    width: \"100%\",\r\n    outline: \"none\",\r\n    padding: \"5px 10px\",\r\n    \"&:focus\": {\r\n      width: \"70%\",\r\n    },\r\n    borderRadius: \"7px\",\r\n    \"& + &\": {\r\n      marginTop: \"16px\",\r\n    },\r\n    \"& > label\": {\r\n      paddingLeft: \"14px\",\r\n    },\r\n  },\r\n  divButtons: {\r\n    width: \"100%\",\r\n    marginTop: \"30px\",\r\n    display: \"flex\",\r\n    justifyContent: \"center\",\r\n    alignItems: \"center\",\r\n  },\r\n  saveButton: {\r\n    width: \"85%\",\r\n    outline: \"none\",\r\n    backgroundColor: \"#4BB543\",\r\n    fontSize: \"18px\",\r\n    fontWeight: 600,\r\n    transition: \"background 0.6s\",\r\n    \"&:hover\": {\r\n      backgroundColor: \"#4BB543AA\",\r\n    },\r\n  },\r\n}));\r\n\r\nexport default withRouter(CadastroFunc);\r\n","C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\meta\\reactHelmet.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\components\\DadosPessoais\\DadosPessoais.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\components\\Header\\Components\\DropDownCartContent.js",["340"],"import React, { useEffect, useContext, useState } from \"react\";\nimport { ClickAwayListener } from \"@material-ui/core\";\nimport { FaShoppingCart } from \"react-icons/fa\";\n\nimport \"./style.css\";\nimport { useHistory } from \"react-router-dom\";\n\nimport api from '../../../services/api';\nimport { LoginContext } from '../../../contexts/LoginContext';\n\n\nexport default function DropDownCartContent(props) {\n  let Subtotal = 0;\n\n  const bucketAWS = process.env.REACT_APP_BUCKET_AWS;\n  const { token } = useContext(LoginContext);\n  const history = useHistory();\n\n  const [productsInCart, setProductsInCart] = useState([]);\n\n  async function getProducts() {\n    const response = await api.get('/cart', {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n    setProductsInCart(response.data);\n  }\n\n  useEffect(() => {\n    try {\n      getProducts();\n    } catch (error) {\n      console.warn(error);\n      alert('Erro ao Buscar carrinho');\n    }\n  }, []);\n\n\n  function handleClickAway() {\n    props.setClickCart(false);\n  }\n\n  return (\n    <ClickAwayListener onClickAway={handleClickAway}>\n      <div className=\"all_window\">\n        <div className=\"blueLine\"></div>\n\n        <div className=\"products\">\n          {\n            productsInCart.length === 0 || productsInCart.length === undefined ?\n              <span style={{color: '#000'}}>Nenhum produto</span>\n            :\n              productsInCart.map((produto) => {\n                Subtotal =\n                  Subtotal + parseFloat(produto.price) * parseFloat(produto.amount);\n        \n                return (\n                  <>\n                    <div className=\"singleProduct\">\n                      <div className=\"photo\">\n                        <img\n                          src={bucketAWS + produto.img_link}\n                          alt={produto.name}\n                          className=\"FotoCamisa\"\n                        />\n                      </div>\n                      <div className=\"texts\">\n                        <h4 className=\"Prod_Title\">{produto.name}</h4>\n                        <div className=\"description\">\n                          <div className=\"pt1\">\n                            <p className=\"size\">Tamanho: {produto.size}</p>\n                          </div>\n                          <div className=\"pt1\">\n                            <p className=\"gender\">Gênero: {produto.gender === 'F'? 'Feminino': 'Masculino'}</p>\n                            <p>R$ {Number(produto.price).toFixed(2)}</p>\n                          </div>\n                          <div className=\"pt2\">\n                            <p className=\"color\">Cor: Branca</p>\n                            <p className=\"total_price\">\n                              {produto.amount} x R$ {Number(produto.price).toFixed(2)}\n                            </p>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                    <hr\n                      style={{\n                        width: \"90%\",\n                        margin: \"-1px auto\",\n                      }}\n                    />\n                  </>\n                );\n            })\n          }\n        </div>\n\n        <div className=\"subtotal_div\">\n          <h5 className=\"subtotal\" style={{ color: \"black\" }}>\n            SUBTOTAL{\" \"}\n          </h5>\n          <h5 className=\"price\" style={{ color: \"black\" }}>\n            R$ {Subtotal},00\n          </h5>\n        </div>\n        <button\n          className=\"stylized_button\"\n          onClick={() => {\n            handleClickAway();\n            history.push(\"/cart\");\n          }}\n        >\n          Ir para o carrinho\n          <FaShoppingCart className=\"cart_icon\" style={{}} />\n        </button>\n      </div>\n    </ClickAwayListener>\n  );\n}\n","C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\components\\ProductCardAdm\\ProductCardAdm.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\components\\SnackbarMessage\\SnackbarMessage.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\components\\ProductModelCardAdm\\index.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\components\\ProductModelCardAdm\\ProductModelCardAdm.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\Pages\\Administrador\\HomeEditable\\HomeEditable.js",["341","342","343","344","345","346","347","348","349","350"],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\App.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Uniform\\frontend-uniformsystem\\src\\services\\auth.js",[],"C:\\Users\\helio\\OneDrive\\Área de Trabalho\\CPE\\Unif