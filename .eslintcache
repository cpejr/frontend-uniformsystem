\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default OrdersAdm;\n","/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Carrinho/Carrinho.js",["375"],"import React, { useEffect, useState, useContext } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport api from '../../services/api';\nimport { LoginContext } from '../../contexts/LoginContext';\nimport './Carrinho.css';\nimport CartProduct from './Components/CartProduct';\nimport ShippingCalc from './Components/ShippingCalc';\n\nfunction Carrinho() {\n  const history = useHistory();\n\n  const { token } = useContext(LoginContext);\n\n\n  const [products, setProducts] = useState([]);\n  const [subTotal, setSubTotal] = useState(0);\n  const [shipping, setShipping] = useState();\n\n  async function handleChangeAmount(number, product_key) {\n    try {\n      const id = products[product_key].product_in_cart_id;\n      const newAmount = products[product_key].amount + number;\n      await api.put(\n        `/cart/${id}`,\n        {\n          amount: newAmount,\n        },\n        {\n          headers: { Authorization: `Bearer ${token}` },\n        },\n      );\n      products[product_key].amount = newAmount;\n      setProducts([...products]);\n    } catch (error) {\n      console.warn(error);\n      alert(error);\n      history.push('Error');\n    }\n  }\n\n  async function handleDelete(product_key) {\n    try {\n      const id = products[product_key].product_in_cart_id;\n      await api.delete(`/cart/${id}`, {\n        headers: { Authorization: `Bearer ${token}` },\n      });\n      products.splice(product_key, 1);\n      setProducts([...products]);\n    } catch (error) {\n      console.warn(error);\n      alert(error);\n      history.push('Error');\n    }\n  }\n\n  async function getProducts() {\n    const response = await api.get('/cart', {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n    setProducts([...response.data]);\n  }\n\n  useEffect(\n    () => {\n      let aux = 0;\n      products.forEach(product => {\n        aux += product.price * product.amount;\n      });\n      setSubTotal(aux);\n    },\n    [products],\n  );\n\n  useEffect(() => {\n    try {\n      getProducts();\n    } catch (error) {\n      console.warn(error);\n      alert('Erro ao Buscar carrinho');\n    }\n  }, []);\n\n  return (\n    <div className=\"cardContainer\">\n      <h1 className=\"cartTitle\">Carrinho</h1>\n      <table className=\"cartTable\">\n        <thead>\n          <tr>\n            <th>Produto</th>\n            <th>Preço Unitário</th>\n            <th>Quantidade</th>\n            <th>Subtotal</th>\n            <th>Excluir</th>\n          </tr>\n        </thead>\n        <tbody>\n          {products.map((product, index) => (\n            <CartProduct\n              key={index}\n              index={index}\n              product={product}\n              changeAmount={handleChangeAmount}\n              handleDelete={handleDelete}\n            />\n          ))}\n          <tr>\n            <td colSpan=\"3\" className=\"subTotal\">\n              Subtotal:{' '}\n            </td>\n            <td colSpan=\"2\" className=\"subTotal\">\n              R${subTotal.toFixed(2).replace('.', ',')}\n            </td>\n          </tr>\n          <tr>\n            <td colSpan=\"3\">\n              <ShippingCalc setShipping={setShipping} />\n            </td>\n            <td colSpan=\"3\">\n              {shipping && `R$${shipping.toFixed(2).replace('.', ',')}`}\n            </td>\n          </tr>\n          <tr>\n            <td colSpan=\"5\" className=\"total\">\n              R$\n              {shipping\n                ? (subTotal + shipping).toFixed(2).replace('.', ',')\n                : subTotal.toFixed(2).replace('.', ',')}\n            </td>\n          </tr>\n        </tbody>\n      </table>\n      {products.length > 0 && (\n        <button\n          className=\"checkoutButton\"\n          onClick={() => history.push('checkout')}\n        >\n          Finalizar Compra\n        </button>\n      )}\n    </div>\n  );\n}\n\nexport default Carrinho;\n",["376","377"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/Footer/Footer.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Checkout/Checkout.js",["378","379","380","381","382","383"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Login/Login.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Administrador/EmployeeAdm/EmployeeAdm.js",["384"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Administrador/EmployeeAdm/CadastroFunc/CadastroFunc.js",["385"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/Header/Mobile_Header/Header.js",[],["386","387"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Administrador/OrdersAdm/EspecificOrderAdm/EspecificOrderAdm.js",["388","389","390","391","392","393","394"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Carrinho/Components/CartProduct.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/ExcludeDialog/ExcludeDialog.js",[],["395","396"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/Endereços/index.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/Toggle/index.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/PopUpProductModel/index.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/Toggle/Toggle.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/Pages/Checkout/PopUpChangeAddress/PopUpChangeAddress.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/Endereços/Endereços.js",[],"/home/bryan/Desktop/UniformSystem/frontend-uniformsystem/src/components/OrderTable/OrderTable.js",[],["397","398"],"/home/bryan/Desktop/UniformSystem/frontend-uniformsyste