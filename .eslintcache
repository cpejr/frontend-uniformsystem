r\n      statusInPortuguese = \"Preparando\";\r\n      break;\r\n    case \"delivering\":\r\n      statusInPortuguese = \"Em entrega\";\r\n      break;\r\n    default:\r\n      break;\r\n  }\r\n  return statusInPortuguese;\r\n}\r\n\r\nfunction CardPedido({ pedido, token }) {\r\n  const [productsFromOrder, setProductsFromOrder] = useState([]);\r\n\r\n  const orderDate = new Date(pedido.created_at);\r\n  const dayOrder = orderDate.getDate();\r\n  const monthOrder = orderDate.getMonth() + 1;\r\n  const yearOrder = orderDate.getFullYear();\r\n\r\n  const statusFormatted = formatOrderStatus(pedido.status);\r\n\r\n  useEffect(() => {\r\n    async function getProductsFromOrder() {\r\n      const response = await api.get(\r\n        `/order/productsfromorder/${pedido.order_id}`,\r\n        {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        }\r\n      );\r\n\r\n      if (response.data) {\r\n        setProductsFromOrder(response.data);\r\n      }\r\n    }\r\n\r\n    getProductsFromOrder();\r\n  }, []);\r\n\r\n  const totalPriceOrder = useMemo(() => {\r\n    let totalAux = 0;\r\n    productsFromOrder.forEach((item) => {\r\n      totalAux += item.product_price * item.amount - item.discount;\r\n      return totalAux;\r\n    });\r\n\r\n    return totalAux;\r\n  }, [productsFromOrder]);\r\n\r\n    return (\r\n    <div>\r\n      <div className=\"pedido\">\r\n        <div className=\"pedidoNumber\">ID: {pedido.order_id}</div>\r\n        <div className=\"pedidoData\">Data do pedido: {dayOrder}/{monthOrder}/{yearOrder}</div>  \r\n        <hr className=\"horizontalLine\"></hr>\r\n        <div className=\"pedidoStatus\">\r\n          <MdInfoOutline style={{fontSize:'22px', marginRight: '7px'}}/>\r\n          <span>Status: {statusFormatted}</span>\r\n        </div>  \r\n        <div className=\"pedidoDestino\">\r\n          <RiTruckFill style={{fontSize:'22px', marginRight: '7px'}}/>\r\n          <span>Destino: {pedido.city}/{pedido.state} - {pedido.zip_code}</span>\r\n        </div>  \r\n        <div className=\"pedidoTotal\">Total: R$ {totalPriceOrder.toFixed(2)}</div>\r\n        {\r\n          pedido.status === \"delivered\" && \r\n          <>\r\n            <span className=\"trackingCode\">Cod. Rastreamento: {pedido.tracking_code}</span>  \r\n            <Button className=\"pedidoBotao2\">\r\n              <a href=\"https://www2.correios.com.br/sistemas/rastreamento/default.cfm\" target=\"_blank\" rel=\"noreferrer\" >\r\n                Acompanhar pedido\r\n              </a>\r\n            </Button>\r\n          </>\r\n        }\r\n      </div>\r\n      <hr className=\"horizontalLine\"></hr>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default CardPedido;\r\n","/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/components/Header/Components/DropDownCartContent.js",["423"],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/components/TabelaFuncionario/TabelaFuncionario.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/components/ProductModelCardAdm/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/components/PopUpForgotPassword/PopUpForgotPassword.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/services/auth.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/Pages/Perfil/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/Pages/Checkout/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/Pages/Error/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/Pages/Administrador/ProductsAdm/RegisterProduct/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/Pages/Administrador/EmployeeAdm/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-uniformsystem/src/Pages/Administrador/EmployeeAdm/EspecificEmployee/index.js",[],"/home/bryan/Desktop/WebProjects/Uniform/frontend-unifo