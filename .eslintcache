ers: { Authorization: `Bearer ${token}` },\r\n        }\r\n      );\r\n      products[product_key].amount = newAmount;\r\n      setProducts([...products]);\r\n    } catch (error) {\r\n      console.warn(error);\r\n      alert(error);\r\n      history.push(\"Error\");\r\n    }\r\n  }\r\n\r\n  async function handleDelete(product_key) {\r\n    try {\r\n      const id = products[product_key].product_in_cart_id;\r\n      await api.delete(`/cart/${id}`, {\r\n        headers: { Authorization: `Bearer ${token}` },\r\n      });\r\n      products.splice(product_key, 1);\r\n      setProducts([...products]);\r\n    } catch (error) {\r\n      console.warn(error);\r\n      alert(error);\r\n      history.push(\"Error\");\r\n    }\r\n  }\r\n\r\n  async function getProducts() {\r\n    const response = await api.get(\"/cart\", {\r\n      headers: { Authorization: `Bearer ${token}` },\r\n    });\r\n    setProducts([...response.data]);\r\n  }\r\n\r\n  useEffect(() => {\r\n    let aux = 0;\r\n    products.forEach((product) => {\r\n      aux += product.price * product.amount;\r\n    });\r\n    setSubTotal(aux);\r\n  }, [products]);\r\n\r\n  useEffect(() => {\r\n    try {\r\n      getProducts();\r\n    } catch (error) {\r\n      console.warn(error);\r\n      alert(\"Erro ao Buscar carrinho\");\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"cardContainer\">\r\n      <MetaData titlePage={meta.titlePage} titleSearch={meta.titleSearch} description={meta.description} keyWords={meta.keyWords} imageUrl={meta.imageUrl} imageAlt={meta.imageAlt} />\r\n      <h1 className=\"cartTitle\">Carrinho</h1>\r\n      <div className=\"cartTable\">\r\n        <table className=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th>Produto</th>\r\n              <th>Preço Unitário</th>\r\n              <th>Quantidade</th>\r\n              <th>Subtotal</th>\r\n              <th>Excluir</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            {products.map((product, index) => (\r\n              <CartProduct\r\n                key={index}\r\n                index={index}\r\n                product={product}\r\n                changeAmount={handleChangeAmount}\r\n                handleDelete={handleDelete}\r\n              />\r\n            ))}\r\n            <tr>\r\n              <td colSpan=\"3\" className=\"subTotal\">\r\n                Subtotal:{\" \"}\r\n              </td>\r\n              <td colSpan=\"2\" className=\"subTotal\">\r\n                R${subTotal.toFixed(2).replace(\".\", \",\")}\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td colSpan=\"3\">\r\n                <ShippingCalc setShipping={setShipping} />\r\n              </td>\r\n              <td colSpan=\"3\">\r\n                {shipping && `R$${shipping.toFixed(2).replace(\".\", \",\")}`}\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td colSpan=\"5\" className=\"total\">\r\n                R$\r\n                {shipping\r\n                  ? (subTotal + shipping).toFixed(2).replace(\".\", \",\")\r\n                  : subTotal.toFixed(2).replace(\".\", \",\")}\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div\r\n        classname=\"buttonSize\"\r\n        style={{\r\n          alignSelf: 'flex-end'\r\n        }}\r\n      >\r\n        {products.length > 0 && (\r\n          <button\r\n            className=\"checkoutButton\"\r\n            onClick={() => history.push(\"checkout\")}\r\n          >\r\n            Finalizar Compra\r\n          </button>\r\n        )}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Carrinho;\r\n",["89","90"],"D:\\Users\\arthu\\Documents\\CPE\\UniformSystem\\frontend-uniformsystem\\src\\Pages\\Contato\\Contato.js",["91"],"D:\\Users\\arthu\\Documents\\CPE\\UniformSystem\\frontend-uniformsystem\\src\\components\\Header\\Components\\DropDownCartContent.js",["92"],"