component={Contato} />\r\n          <Route path=\"/cadastro\" export component={Cadastro} />\r\n  \r\n          {/* A página abaixo é para que se algo existir uma página que não está no routes, apracer o seguinte. */}\r\n          <Route path='*' exact component={Error} />\r\n        </Switch>\r\n        {(user !== null && user[0].user_type === \"adm\") ?\r\n          <Link to=\"/adm/home\" style={{position: 'fixed', right: '1vw', bottom: '11vh'}}>\r\n            <Fab color=\"primary\" aria-label=\"edit\">\r\n              <EditIcon />\r\n            </Fab>\r\n          </Link>\r\n          : null}\r\n        <Footer />\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nfunction AdmRoutes() {\r\n  const { user } = useContext(LoginContext);\r\n\r\n  if (user === \"notYet\") return <Loading/>;\r\n  if (user === null || user.user_type === \"adm\") return <Redirect to=\"/adm/home\" />;\r\n  else\r\n  return (\r\n    <div>\r\n      <HeaderAdm />\r\n      <SidebarAdm>\r\n        <Switch>\r\n          <PrivateADMRoute\r\n            path=\"/adm/home\"\r\n            component={HomeEditable}\r\n          />\r\n          <PrivateADMOrEmployeeRoute\r\n            path=\"/adm/pedidos\"\r\n            exact\r\n            component={OrdersAdm}\r\n          />\r\n          <PrivateADMOrEmployeeRoute\r\n            path=\"/adm/pedidoespecifico\"\r\n            export\r\n            component={EspecificOrderAdm}\r\n          />\r\n          <PrivateADMRoute \r\n            path=\"/adm/produtos\" \r\n            exact\r\n            component={ProductsAdm} \r\n          />\r\n          <PrivateADMRoute\r\n            path=\"/adm/produtos/cadastro\"\r\n            export\r\n            component={RegisterProduct}\r\n          />\r\n          <PrivateADMRoute\r\n            path=\"/adm/produtos/:product_id\"\r\n            export\r\n            component={EditProduct}\r\n          />\r\n          <PrivateADMRoute \r\n            path=\"/adm/funcionarios\" \r\n            export\r\n            exact \r\n            component={EmployeeAdm} \r\n          />\r\n          <PrivateADMRoute\r\n            path=\"/adm/funcionarios/cadastro\"\r\n            export\r\n            component={CadastroFunc}\r\n          />\r\n          <PrivateADMRoute\r\n            path=\"/adm/funcionario/:id\"\r\n            export\r\n            component={EspecificEmployee}\r\n          />\r\n          <Route path=\"*\" exact={true} component={Error} />\r\n        </Switch>\r\n      </SidebarAdm>\r\n      {(user !== null && user[0].user_type === \"adm\") ?\r\n        <Link to=\"/\" style={{position: 'fixed', right: '1vw', bottom: '11vh'}}>\r\n          <Fab color=\"primary\" aria-label=\"home\">\r\n            <HomeIcon />\r\n          </Fab>\r\n        </Link>\r\n        : null}\r\n      <FooterAdm />\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction Loading(props) {\r\n  const { user } = useContext(LoginContext);\r\n  console.log(\"UseEffect Loading em Loading: \", user);\r\n  if (user.type === \"adm\") return <Redirect to=\"/adm/home\" />;\r\n  if (user === null || user === \"notYet\") return <Redirect to=\"/login\" />;\r\n  else\r\n    return (\r\n      <div className=\"loading\">\r\n        <div className=\"loading-logo\">\r\n          <ClipLoader size={100} color={\"#123abc\"} loading={true} />\r\n        </div>\r\n      </div>\r\n    );\r\n}\r\n",["321","322"],"C:\\Users\\Felipe Vidotti\\Desktop\\Projetos\\frontend-uniformsystem\\src\\Pages\\EditarPerfil\\index.js",[],"C:\\Users\\Felipe Vidotti\\Desktop\\Projetos\\frontend-uniformsystem\\src\\Pages\\Home\\index.js",[],"C:\\Users\\Felipe Vidotti\\Desktop\\Projetos\\frontend-uniformsystem\\src\\Pages\\Carrinho\\index.js",[],["323","324"],"C:\\Users\\Felipe Vidotti\\Desktop\\Projetos\\frontend-uniformsystem\\src\\Pages\\Login\\index.js",[],"C:\\Users\\Felipe Vidotti\\Desktop\\Projetos\\frontend-uniformsystem\\src\\Pages\\Administrador\\HomeEditable\\index.js",[],"C:\\Users\\Felipe Vidotti\\Desktop\\Projetos\\frontend-uniformsys