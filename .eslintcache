}`} className=\"buyButton\">COMPRAR</Link>\r\n            </div>\r\n        </Link>\r\n    )\r\n}\r\nexport default ProductCard","C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\components\\ProductCardAdm\\ProductCardAdm.js",["295","296"],"import React from 'react';\r\nimport './ProductCardAdm.css';\r\nimport { Link } from 'react-router-dom';\r\nimport camisa from '../../Assets/camisa.jpg';\r\nimport { MdCreate } from 'react-icons/md';\r\nimport { size } from 'lodash';\r\n\r\nfunction ProductCardAdm(props) {\r\n\r\n    return (\r\n        <Link className=\"product-adm\" to={`/adm/produtos/${props.product.product_id}`}>\r\n            <div className=\"edit-button\">\r\n                <MdCreate color=\"black\" size=\"24px\" />\r\n            </div>\r\n            <div className=\"product-content\">\r\n                <div className=\"product-img\">\r\n                    <img className=\"img-product\" src={camisa} />\r\n                </div>\r\n                <div className=\"product-name\">{props.product.name}</div>\r\n                <div className=\"product-adm-price\">{(props.product.models.price).toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' })}</div>\r\n            </div>\r\n        </Link>\r\n    )\r\n}\r\n\r\nexport default ProductCardAdm","C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\Pages\\Login\\Login.js",[],"C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\Pages\\Administrador\\ProductsAdm\\EditProduct\\EditProduct.js",["297","298","299"],"C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\Pages\\Administrador\\EmployeeAdm\\CadastroFunc\\CadastroFunc.js",["300"],"C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\services\\auth.js",[],"C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\Pages\\Produto\\index.js",[],"C:\\Users\\rapha\\Documents\\UFMG\\CPE\\frontend-uniformsystem\\src\\Pages\\Administrador\\EmployeeAdm\\Cada